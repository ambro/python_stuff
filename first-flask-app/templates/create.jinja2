{% extends 'base.jinja2' %}

{% block body_content %}
<h1>Create post</h1>
<form method="post">
    <div>
        <label for="title">Title</label>
        <input type="text" name="title" />
    </div>
    <div>
        <label for="content">Content</label>
        <textarea type="text" name="content"></textarea>
    </div>
    <div>
        <input type="submit" />
    </div>
</form>
{% endblock %}